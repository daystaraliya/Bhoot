// Online C++ compiler to run C++ program online
#include <iostream>
#include<cmath>
using namespace std;
int sum(int n);

int main() {
int n;
cout<<"Enter";
cin>>n;
cout<<"Sum is "<<sum(n)<<endl;
}
int sum(int n) {
    if(n==1) {
        return 1;
    }
    int sumn1=sum(n-1);
    int sumn=sum(n-1)+n;
    return sumn;
}


